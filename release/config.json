{
    "RunEnv": 0,
    "Version": "0.8.7",
    "NeuronId": "ChronusProxy",
    "NeuronMail": "adumandix@gmail.com",
    "AutorunConfig": {
        "ADProxy": true,
        "SDExampleInterface": true
    },
    "Proxy": {
        "ProxyHub": {
            "TCP": {},
            "TCP2UDP": {},
            "UART2UDP": {},
            "UDP": {},
            "UDP2TCP": {}
        }
    },
    "File": {
        "Chmod": 502,
        "TempPath": "/static/temp/"
    },
    "HTTPRequest": {
        "DefaultHeader": {
            "User-Agent": [
                "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_12_5) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/64.0.3282.186 Safari/537.36"
            ]
        }
    },
    "HTTPServer": {
        "Host": "0.0.0.0",
        "Port": 8800,
        "StaticPath": "/static",
        "UploadPath": "/static/upload",
        "XPoweredBy": "Chronus Express",
        "ACAO": false
    },
    "HTTPS": {
        "Open": false,
        "TLSPort": 8443,
        "TLSCertPath": "/tls/tls"
    },
    "WSParam": {
        "Interval": 120000,
        "BufferSize": 2097152
    },
    "TCPParam": {
        "Interval": 120000,
        "BufferSize": 8192
    },
    "UDPParam": {
        "MaxLen": 1,
        "Interval": 120000,
        "BufferSize": 8192
    },
    "UartParam": {
        "Interval": 120000,
        "BufferSize": 8192
    },
    "Interval": {
        "HZ25TimerInterval": 40,
        "HZ8TimerInterval": 125,
        "HZ4TimerInterval": 250,
        "HZ2TimerInterval": 500,
        "HZ1TimerInterval": 1000,
        "CommanderTimerInterval": 100,
        "LooperTimerInterval": 3000,
        "SystemTimerInterval": 30000,
        "RetryTimerInterval": 5000,
        "MicroMsgTokenInterval": 7200000
    }
}